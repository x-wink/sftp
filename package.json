{
    "name": "wink-vscode-sftp",
    "displayName": "wink-sftp",
    "description": "SFTP远程工具",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "wink-vscode-sftp.helloWorld",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "watch": "tsc -watch -p ./",
        "test": "node ./out/test/runTest.js",
        "build": "tsc && rollup --bundleConfigAsCjs --config out/rollup.config.js",
        "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
        "release": "bumpp package.json --commit \"release: v%s\" --push --all --tag && pnpm run build && pnpm publish --access public",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "prettier": "prettier --write ./**/*.{vue,ts,tsx,js,jsx,css,less,json,md,html}",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.4.1",
        "@commitlint/config-conventional": "^17.4.0",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-commonjs": "^25.0.0",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.0.2",
        "@rollup/plugin-typescript": "^11.1.1",
        "@xwink/eslint-config": "^1.0.0",
        "@types/vscode": "^1.78.0",
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@types/ssh2": "^1.11.11",
        "@typescript-eslint/eslint-plugin": "^5.59.1",
        "@typescript-eslint/parser": "^5.59.1",
        "bumpp": "^9.1.0",
        "conventional-changelog-cli": "^2.2.2",
        "eslint": "^8.39.0",
        "glob": "^8.1.0",
        "husky": "^8.0.3",
        "lint-staged": "^13.1.0",
        "mocha": "^10.2.0",
        "prettier": "^2.8.4",
        "rollup": "^3.23.0",
        "typescript": "^5.0.4",
        "@vscode/test-electron": "^2.3.0"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,vue}": [
            "npm run lint"
        ],
        "*.{vue,ts,tsx,js,jsx,css,less,json,md,html}": [
            "npm run prettier"
        ]
    },
    "dependencies": {
        "ssh2": "^1.13.0"
    }
}
