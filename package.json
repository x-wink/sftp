{
    "name": "@xwink/sftp",
    "version": "1.0.0",
    "description": "远程部署SFTP工具",
    "keywords": [
        "ssh",
        "sftp",
        "remote publish",
        "command"
    ],
    "author": {
        "name": "向文可",
        "email": "13202090601@163.com",
        "url": "https://github.com/x-wink"
    },
    "repository": {
        "url": "https://github.com/x-wink/wink-sftp"
    },
    "private": false,
    "files": [
        "dist/src",
        "dist/index.js"
    ],
    "main": "dist/index.js",
    "bin": {
        "wink-sftp": "dist/index.js"
    },
    "types": "dist/src/index.d.ts",
    "browser": "dist/index.js",
    "exports": {
        ".": {
            "types": "./types/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        }
    },
    "scripts": {
        "dev": "ts-node src/index.ts -c sftp.json",
        "build": "set NODE_OPTIONS=--openssl-legacy-provider && tsc && ncc build ./src/index.ts -o ./dist",
        "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
        "release": "bumpp package.json --commit \"release: v%s\" --push --all --tag && pnpm run build && pnpm publish --access public",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "prettier": "prettier --write ./**/*.{vue,ts,tsx,js,jsx,css,less,json,md,html}",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.4.1",
        "@commitlint/config-conventional": "^17.4.0",
        "@saatana97/eslint-config": "^1.0.0",
        "@types/node": "^20.2.3",
        "@types/ssh2": "^1.11.11",
        "@vercel/ncc": "^0.36.1",
        "bumpp": "^9.1.0",
        "conventional-changelog-cli": "^2.2.2",
        "eslint": "^8.35.0",
        "husky": "^8.0.3",
        "less": "^4.1.3",
        "lint-staged": "^13.1.0",
        "prettier": "^2.8.4",
        "ts-node": "^10.9.1",
        "typescript": "~5.0.4"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,vue}": [
            "npm run lint"
        ],
        "*.{vue,ts,tsx,js,jsx,css,less,json,md,html}": [
            "npm run prettier"
        ]
    },
    "dependencies": {
        "commander": "^10.0.1",
        "ssh2": "^1.13.0"
    }
}
